{
  "manifest_version": 3,
  "name": "Price Tracker",
  "version": "1.0",
  "description": "Śledzi zmiany cen produktów.",
  "permissions": [
    "storage",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png"
  }
}
